<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.persAssistant.shopping_list">

    <application
        android:name=".presentation.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Shopping_list">
        <activity android:name=".presentation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.category.CreatorCategoryActivity" />
        <activity android:name=".presentation.category.EditorCategoryActivity" />
        <activity android:name=".presentation.purchaseList.CreatorPurchaseListActivity" />
        <activity android:name=".presentation.purchaseList.EditorPurchaseListActivity" />
    </application>

</manifest>